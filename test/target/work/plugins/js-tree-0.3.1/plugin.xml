<plugin name='js-tree' version='0.3.1' grailsVersion='1.3.7 &gt; *'>
  <author>Jakob Munih</author>
  <authorEmail>munih@parsek.net</authorEmail>
  <title>JsTree</title>
  <description>\
Provieds jsTree jQuery lib resources. 
jsTree is a javascript based, cross browser tree component. It is packaged as a jQuery plugin.
</description>
  <documentation>http://grails.org/plugin/js-tree</documentation>
  <type>JsTreeGrailsPlugin</type>
  <resources>
    <resource>JsTreePluginResources</resource>
    <resource>com.jsTree.JsTreeTagLib</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='jquery' version='1.7.1' />
    </runtime>
  </plugins>
  <runtimePluginRequirements>
    <plugin name='jquery' version='1.4 &gt; *' />
  </runtimePluginRequirements>
  <behavior />
</plugin>